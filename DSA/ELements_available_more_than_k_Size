class HelloWorld {
    public static void main(String[] args) {
        int arr[]={1,2,2,3,3,3,4,4,5,5,5,5,6,7,8};
        int n = arr.length;
        int count =0;
        int k =3;
        for(int i=0;i<n;i++)
        {
            if(arr[i]>=k)
            {
                count++;
           }
        }
        System.out.println(count); 
    }
}