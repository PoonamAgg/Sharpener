class HelloWorld {
    public static void main(String[] args) {
        int arr[]={3,0,1,2,6,4};
        int arr_sum=0;
        int arr_ind =0;
        int n = arr.length;
        for(int i =0;i<n;i++)
        {
            arr_sum=arr_sum+arr[i];
        }
        for(int j =1;j<=n;j++)
        {
            arr_ind = arr_ind+j;
        }
        int diff = arr_ind-arr_sum;
        System.out.println(diff);
    }
}